<%= form_for @cuisine do |f| %>
  <%= f.hidden_field :bitter, value: @similar_profile.bitter %>
  <%= f.hidden_field :earthy, value: @similar_profile.earthy %>
  <%= f.hidden_field :grassy, value: @similar_profile.grassy %>
  <%= f.hidden_field :licorice, value: @similar_profile.licorice %>
  <%= f.hidden_field :nutty, value: @similar_profile.nutty %>
  <%= f.hidden_field :peppery, value: @similar_profile.peppery %>
  <%= f.hidden_field :sour, value: @similar_profile.sour %>
  <%= f.hidden_field :spicy, value: @similar_profile.spicy %>
  <%= f.hidden_field :sweet, value: @similar_profile.sweet %>
  <%= f.hidden_field :woody, value: @similar_profile.woody %>
  <%= f.submit "Find the Most Similar Cuisine Style", id: "taste-profile" %>
<% end %>
